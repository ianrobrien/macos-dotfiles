#!/bin/bash
#Generated by JetBrains Toolbox 1.22.10970 at 2022-03-09T19:41:31.717410

declare -a ideargs=()
declare -- wait=""

for o in "$@"; do
  if [[ "$o" = "--wait" || "$o" = "-w" ]]; then
    wait="-W"
    o="--wait"
  fi
  if [[ "$o" =~ " " ]]; then
    ideargs+=("\"$o\"")
  else
    ideargs+=("$o")
  fi
done

open -na "/Applications/PyCharm.app/Contents/MacOS/pycharm" $wait --args "${ideargs[@]}"
