#!/bin/bash
#Generated by JetBrains Toolbox 1.22.10970 at 2022-03-09T19:46:14.151925

declare -a ideargs=()
declare -- wait=""

for o in "$@"; do
  if [[ "$o" = "--wait" || "$o" = "-w" ]]; then
    wait="-W"
    o="--wait"
  fi
  if [[ "$o" =~ " " ]]; then
    ideargs+=("\"$o\"")
  else
    ideargs+=("$o")
  fi
done

open -na "/Applications/IntelliJ IDEA.app/Contents/MacOS/idea" $wait --args "${ideargs[@]}"